<mvc:View
    controllerName="template.controller.Detail"
    displayBlock="true"
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc"
>
    <Page title="Detalle del Usuario">
        <content>
            <VBox>
                <Text text="{/selectedUser/name}" />
                <Text text="{/selectedUser/email}" />
                <Text text="{/selectedUser/phone}" />
                <Text text="{/selectedUser/website}" />
            </VBox>
        </content>
    </Page>
</mvc:View>
